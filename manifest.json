{
  "manifest_version": 3,
  "name": "Inspirational Quotes",
  "version": "1.0",
  "description": "Inspiring quotes for all to keep motivated and focused.",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "permissions": [
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "96": "icon.png",
    "128": "icon.png"
  }
}
